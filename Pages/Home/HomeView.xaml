<UserControl x:Class="ErpAlgerie.Pages.Home.HomeView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ErpAlgerie.Pages.Home"
             mc:Ignorable="d"  Background="Black"
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Margin="0">
        <Grid.Background>
            <ImageBrush Opacity=".4" ImageSource="/ErpAlgerie;component/Images/bgbig.jpg"/>
        </Grid.Background>
        <ContentControl HorizontalAlignment="Center"  Margin="50" Content="{Binding MasterPanel, Mode=OneWay,
            UpdateSourceTrigger=PropertyChanged}"/>
    </Grid>
</UserControl>
